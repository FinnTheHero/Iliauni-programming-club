---
import { SITE_DESCRIPTION, SITE_TITLE } from "../config";
import MainLayout from "../layouts/MainLayout.astro";
---

<MainLayout
	title={`404 Not Found | ${SITE_TITLE}`}
	description={SITE_DESCRIPTION}
>
	<div class="flex items-center justify-center flex-col py-16">
		<h1 style="color: red;">404</h1>
		<p>
			The page you are looking for is under development or does not exists.
		</p>
		<div class="py-8"><a href="/" class="custom-link">[Go Back]</a></div>
	</div>
</MainLayout>
